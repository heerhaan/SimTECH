<MudIcon Icon="@Icon" Style="@_style" />

@code {
    [Parameter] public string Icon { get;set;}
    [Parameter] public string Colour { get; set; }
    [Parameter] public string Accent { get; set; }

    private string _style
    {
        get
        {
            return $"color:{Colour}; fill:{Accent}";
        }
    }
}
