@inherits LayoutComponentBase
@layout MainLayout

<MudLayout>
    <MudPaper Class="mt-10 mb-10" Elevation="15">
        <MudToolBar>
            <MudButton Href="/" Variant="Variant.Filled" StartIcon="@Icons.Material.Outlined.SettingsInputComponent" Color="Color.Inherit" IconColor="Color.Default" Size="Size.Large">Engines</MudButton>
            <MudButton Href="/" Variant="Variant.Filled" StartIcon="@Icons.Material.Outlined.Power" Color="Color.Inherit" IconColor="Color.Tertiary" Size="Size.Large">Suppliers</MudButton>
            <MudButton Href="/" Variant="Variant.Filled" StartIcon="@Icons.Material.Outlined.ModeStandby" Color="Color.Inherit" IconColor="Color.Secondary" Size="Size.Large">Tyres</MudButton>
            <MudButton Href="/" Variant="Variant.Filled" StartIcon="@Icons.Material.Outlined.Extension" Color="Color.Inherit" IconColor="Color.Primary" Size="Size.Large">Strategies</MudButton>
        </MudToolBar>
    </MudPaper>
    <MudContainer>
        @Body
    </MudContainer>
</MudLayout>
