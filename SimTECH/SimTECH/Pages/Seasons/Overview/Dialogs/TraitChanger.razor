<MudDialog>
    <DialogContent>
        <MudStack Row="true" AlignItems="AlignItems.Center" Class="px-0 pb-2">
            <MudText Typo="Typo.h5" Color="Color.Secondary">Change traits</MudText>
            <MudSpacer />
            <MudButtonGroup Variant="Variant.Text" Color="Color.Secondary">
                <MudIconButton Icon="@Icons.Material.Filled.Close" OnClick="@(() => TraitDialog?.Close())" />
            </MudButtonGroup>
        </MudStack>
        <MudPaper Class="px-2">
        </MudPaper>
    </DialogContent>
</MudDialog>

@code {
    [CascadingParameter]
    protected MudDialogInstance? TraitDialog { get; set; }


}
